<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="621.0" prefWidth="625.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.travelexpertsjava.TravelExpertsJavaController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label text="Choose a Table" />
   <ComboBox fx:id="cboSelect" prefHeight="25.0" prefWidth="254.0" />
   <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0">
      <children>
         <Button fx:id="btnAdd" mnemonicParsing="false" text="Add" />
         <Button fx:id="btnEdit" mnemonicParsing="false" text="Edit" />
         <Button fx:id="btnDelete" mnemonicParsing="false" text="Delete" />
         <Button fx:id="btnCancel" mnemonicParsing="false" text="Cancel" />
      </children>
   </HBox>

    <Label fx:id="welcomeText" />
   <TableView fx:id="tvTable" prefHeight="489.0" prefWidth="492.0">
     <columns>
       <TableColumn prefWidth="75.0" text="C1" />
       <TableColumn prefWidth="75.0" text="C2" />
     </columns>
   </TableView>
</VBox>
